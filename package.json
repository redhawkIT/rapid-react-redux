{
  "name": "rapid-react-redux",
  "version": "1.0.0",
  "description": "Dead-simple boilerplate for beginners to learn flux architecture, compete in hackathons, and build MVPs for startups.",
  "keywords": [
    "react",
    "redux",
    "hot-reloading",
    "boilerplate",
    "es6",
    "decorators",
    "sass",
    "react-md"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RcKeller/rapid-react-redux"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --inline --hot --history-api-fallback --host localhost --port 8080",
    "build": "webpack -p --env.production --config ./webpack/webpack.config.js",
    "test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
    "test-watch": "npm test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "Ryan Keller",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "15.5.0",
    "react-dom": "15.5.0",
    "react-helmet": "^5.1.3",
    "react-md": "^1.0.18",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-responsive": "^4.3.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^3.5.1",
    "webpack-dev-server": "^2.7.1"
  }
}
